{
    "name": "mikofb/mikdoc",
    "description": "A multi-user laravel package for files and folder management.",
    "keywords": ["laravel", "file", "documents", "management", "mulit-user"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikofb",
            "email": "broukoffifranckmichael@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.8"
    },
    "require-dev":{
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Mikofb\\Mikdoc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mikofb\\Mikdoc\\MikdocServiceProvider"
            ],
            "aliases": {
                "Mikdoc": "Mikofb\\Mikdoc\\Facades\\Mikdoc"
            }
        }
    }
}
